<%- model_class = Bdsensor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome_sensor) %>:</strong></dt>
  <dd><%= @bdsensor.nome_sensor %></dd>
  <dt><strong><%= model_class.human_attribute_name(:time_read) %>:</strong></dt>
  <dd><%= @bdsensor.time_read %></dd>
  <dt><strong><%= model_class.human_attribute_name(:LI) %>:</strong></dt>
  <dd><%= @bdsensor.LI %></dd>
  <dt><strong><%= model_class.human_attribute_name(:LS) %>:</strong></dt>
  <dd><%= @bdsensor.LS %></dd>
  <dt><strong><%= model_class.human_attribute_name(:arg1) %>:</strong></dt>
  <dd><%= @bdsensor.arg1 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:arg2) %>:</strong></dt>
  <dd><%= @bdsensor.arg2 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:arg3) %>:</strong></dt>
  <dd><%= @bdsensor.arg3 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:arg4) %>:</strong></dt>
  <dd><%= @bdsensor.arg4 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:arg5) %>:</strong></dt>
  <dd><%= @bdsensor.arg5 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:flag_notificacao) %>:</strong></dt>
  <dd><%= @bdsensor.flag_notificacao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:flag_rearme) %>:</strong></dt>
  <dd><%= @bdsensor.flag_rearme %></dd>
  <dt><strong><%= model_class.human_attribute_name(:flag_mantec) %>:</strong></dt>
  <dd><%= @bdsensor.flag_mantec %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ativo_inativo) %>:</strong></dt>
  <dd><%= @bdsensor.ativo_inativo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bdtipo_id) %>:</strong></dt>
  <dd><%= @bdsensor.bdtipo_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bdcliente_id) %>:</strong></dt>
  <dd><%= @bdsensor.bdcliente_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              bdsensors_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_bdsensor_path(@bdsensor), :class => 'btn btn-default' %>
<%= button_to t('.destroy', :default => t("helpers.links.destroy")),
              bdsensor_path(@bdsensor),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
